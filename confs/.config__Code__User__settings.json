{
"jshint.options": {"esversion": 8},
"rust-client.channel": "stable",
"rust.rustup": {"toolchain": "stable-x86_64-unknown-linux-gnu"},
"rust.rustLangSrcPath": "/home/chris/.rustup/toolchains/stable-x86_64-unknown-linux-gnu/lib/rustlib/src/rust/src",
"rust.actionOnSave": "clippy",
"rust.showOutput": false,
"rust.cargoHomePath": "/home/chris/.cargo",
"rust.racerPath": "/home/chris/.cargo/bin/racer",
"rust.rustfmtPath": "/home/chris/.cargo/bin/rustfmt",

"files.trimTrailingWhitespace": true,
"python.linting.pylintArgs": ["--enable=python3"],
"editor.fontFamily": "'Ubuntu Mono'",
"editor.links": false,
"editor.renderWhitespace": "none",
"editor.rulers": [88],
"files.exclude": {
    "**/target": true,  // Rust
    "**/codegen": true,  // Rust
    "**/Rocket/examples": true,  // Rust
    "**/*.pyc": true,
    "**/.cache": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.idea": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/.DS_Store": true
},
"gitlens.advanced.messages": {"suppressShowKeyBindingsNotice": true},
"gitlens.blame.highlight.enabled": false,
"gitlens.codeLens.enabled": false,
"gitlens.currentLine.dateFormat": "YYYY-MM-DD hh:mm:ss",
"gitlens.currentLine.enabled": false,
"python.venvPath": "~/venv/",
"python.venvFolders": [".virtualenvs"],
"window.title": "${dirty}${activeEditorLong}${separator}[${rootName}]",
"workbench.iconTheme": null,
"workbench.sideBar.location": "left",
"explorer.confirmDragAndDrop": false,
"editor.cursorSmoothCaretAnimation": "on",
"dart.closingLabels": false,
"workbench.activityBar.visible": true,
"editor.fontSize": 18,
"editor.lineHeight": 24,
"python.formatting.provider": "black",
"python.formatting.yapfPath": "yapfautopep8",
"files.associations": {
    "*.html": "html"
},
"python.testing.autoTestDiscoverOnSaveEnabled": false,
"window.menuBarVisibility": "toggle",
"files.autoSave": "off",
"[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
},
"editor.suggestSelection": "first",
"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
"python.jediEnabled": false,
"update.mode": "none",
"window.customMenuBarAltFocus": false,
"debug.allowBreakpointsEverywhere": true,
"rust-analyzer.inlayHints.enable": false,
"python.languageServer": "Pylance",
"java.semanticHighlighting.enabled": true,
"workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
},
"notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
},
"[python]": {
    "editor.formatOnType": true
},
"redhat.telemetry.enabled": false
}
